    <section class="appointment">
        <div class="container">
            <div class="appointment-inner">
                <div class="appointment-block row">
                    <div class=" col-lg-6 col-12  col-md-12 col-sm-12 appointment-form">
                        <h1 id="book-form">Book An Appointment</h1>
                        {{> messages}}
                        <form method="POST" action="/appointment">
                            <input type="text" id="name" name="fullname" placeholder="Your Fullname*">
                            <input type="email" id="email" placeholder="Your Email*" name="email">
                            <input type="tel" id="phone" placeholder="Phone Number*" name="phone">
                            <input
                                type="text"
                                class="form-control"
                                id="datepicker"
                                placeholder="Choose Date"
                                autocomplete="off"
                                readonly="readonly"
                                name="date"
                            />
                            <select id="time" name="time">
                                <option value="">Choose Time...</option>
                            </select>
                            <select name="place" id="place">
                                <option value="" selected>Choose Department...</option>
                                <option value="Brain-althim">Brain-althim</option>
                                <option value="Heart-beat">Heart-beat</option>
                                <option value="Heart-beat">Blood-test</option>
                            </select>
                            <textarea name="message" id="msg" cols="40" rows="6" placeholder="Message*"></textarea>
                            <button class="form-btn" type="submit">Make Your Appointment</button>
                        </form>
                    </div>
                    <div class="col-lg-6 col-12 col-md-12  appointment-img">
                       <img src="/static/assets/img/appoint-img.jpeg" alt="">
                    </div>
                </div>
            </div>
        </div>
    </section>



